{
  "name": "zasso",
  "version": "0.1.0",
  "bin": {
    "zasso": "bin/zasso.js"
  },
  "scripts": {
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "eslint --fix \"**\"",
    "fix:prettier": "prettier --write \"**\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"**\"",
    "lint:prettier": "prettier --check \"**\"",
    "build": "tsc",
    "watch": "tsc -w",
    "pretest": "npm run build",
    "test": "jest",
    "cdk": "cdk",
    "predeploy:lambda": "npm run build",
    "deploy:lambda": "cdk deploy ZassoLambdaStack*",
    "predeploy": "npm run build",
    "deploy": "cdk deploy Zasso*",
    "destroy": "cdk destroy Zasso*"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.38.0",
    "@aws-cdk/aws-chatbot": "^1.38.0",
    "@aws-cdk/aws-cloudformation": "^1.38.0",
    "@aws-cdk/aws-events": "^1.38.0",
    "@aws-cdk/aws-events-targets": "^1.38.0",
    "@aws-cdk/aws-iam": "^1.38.0",
    "@aws-cdk/aws-lambda": "^1.38.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.38.0",
    "@aws-cdk/aws-stepfunctions": "^1.38.0",
    "@aws-cdk/aws-stepfunctions-tasks": "^1.38.0",
    "@types/aws-lambda": "^8.10.51",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.2.1",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "10.17.5",
    "@types/request-promise": "^4.1.46",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "aws-cdk": "^1.38.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "fs-extra": "^9.0.0",
    "jest": "^25.5.4",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.0",
    "ts-node": "^8.10.1",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/core": "^1.38.0",
    "@holiday-jp/holiday_jp": "^2.2.3",
    "aws-sdk": "^2.672.0",
    "moment-timezone": "^0.5.28",
    "parcel": "^2.0.0-nightly.255",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "source-map-support": "^0.5.16"
  }
}
