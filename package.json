{
  "name": "zasso",
  "version": "0.1.1",
  "bin": {
    "zasso": "bin/zasso.js"
  },
  "scripts": {
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "eslint --fix \"**\"",
    "fix:prettier": "prettier --write \"**\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"**\"",
    "lint:prettier": "prettier --check \"**\"",
    "build": "tsc",
    "watch": "tsc -w",
    "pretest": "npm run build",
    "test": "jest",
    "cdk": "cdk",
    "predeploy:lambda": "npm run build",
    "deploy:lambda": "cdk deploy lambda-stack",
    "predeploy": "npm run build",
    "deploy": "cdk deploy *stack",
    "destroy": "cdk destroy *stack"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.49.1",
    "@aws-cdk/aws-chatbot": "^1.49.1",
    "@aws-cdk/aws-cloudformation": "^1.49.1",
    "@aws-cdk/aws-events": "^1.49.1",
    "@aws-cdk/aws-events-targets": "^1.49.1",
    "@aws-cdk/aws-iam": "^1.49.1",
    "@aws-cdk/aws-lambda": "^1.49.1",
    "@aws-cdk/aws-lambda-nodejs": "^1.49.1",
    "@aws-cdk/aws-stepfunctions": "^1.49.1",
    "@aws-cdk/aws-stepfunctions-tasks": "^1.49.1",
    "@types/aws-lambda": "^8.10.57",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.3",
    "@types/moment-timezone": "0.5.13",
    "@types/node": "14.0.14",
    "@types/request-promise": "^4.1.46",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "aws-cdk": "^1.49.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fs-extra": "^9.0.1",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "~3.9.6"
  },
  "dependencies": {
    "@aws-cdk/core": "^1.49.1",
    "@holiday-jp/holiday_jp": "^2.2.3",
    "aws-sdk": "^2.709.0",
    "env-var": "^6.3.0",
    "moment-timezone": "0.5.13",
    "parcel": "^2.0.0-nightly.255",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "source-map-support": "^0.5.19"
